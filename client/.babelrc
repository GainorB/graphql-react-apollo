{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    [
      "styled-jsx/babel",
      {
        "optimizeForSpeed": true
      }
    ],
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ],
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}